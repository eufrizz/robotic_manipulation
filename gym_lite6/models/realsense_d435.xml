<mujoco model="d435i">
    <default>
      <default class="realsense_d435i_rgb_1920">
        <!-- <camera focal="1.88e-3 1.88e-3" resolution="1920 1080" sensorsize="2.7288e-3 1.5498e-3"/> -->
        <camera fovy="43.04" resolution="1920 1080"/>
      </default>
      <default class="realsense_d435i_rgb_1280">
        <camera fovy="43.04" resolution="1280 720"/>
      </default>
      <default class="realsense_d435i_rgb_640">
        <camera fovy="43.04" resolution="640 480"/>
      </default>
      <!-- The stereo options seem to be a pain in the ass to get working as you need to create a scene object and call mjv_updateScene  -->
      <default class="realsense_d435i_stereo_1280">
        <camera fovy="58.55" resolution="1280 720" ipd="0.0501560494303703"/>
      </default>
      <default class="realsense_d435i_stereo_848">
        <camera fovy="58.86" resolution="848 480" ipd="0.0501560494303703"/>
      </default>
      <default class="realsense_d435i_stereo_640">
        <camera fovy="63.84" resolution="640 480" ipd="0.0501560494303703"/>
      </default>
    </default>
</mujoco>